{
	"name":"ixolit/dislo-sdk",
	"license": "MIT",
	"type": "library",
	"description": "Client library for Dislo APIs",
	"autoload": {
		"psr-4": {
			"Ixolit\\Dislo\\": "./src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ixolit\\Dislo\\": "./tests"
		}
	},
	"suggest": {
		"ixolit/dislo-sdk-http-guzzle": "Guzzle connector for the Dislo SDK"
	},
	"require": {
		"ixolit/dislo-sdk-http": "dev-beta",
		"psr/http-message": "^1.0"
	},
	"require-dev": {
		"ixolit/dislo-sdk-http-guzzle": "^1.0",
		"phpunit/phpunit": "^4.8",
		"phpmd/phpmd": "^2.4",
		"phing/phing": "^2.14",
		"apigen/apigen": "^4.1",
		"fzaninotto/faker": "^1.9@dev",
		"mockery/mockery": "1.3.x-dev"
	},
	"minimum-stability": "dev"
}
